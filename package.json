{
    "name": "osm-pbf-parser-node",
    "version": "1.1.0",
    "description": "Streaming OpenStreetMap PBF parser for Node.js",
    "main": "dist/bundle.js",
    "types": "dist/bundle.d.ts",
    "type": "module",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "rollup -c ",
        "pbf": "pnpm pbf:file && pnpm pbf:osm",
        "pbf:file": "pbf proto/fileformat.proto --typescript > proto/fileformat.ts",
        "pbf:osm": "pbf proto/osmformat.proto --typescript > proto/osmformat.ts"
    },
    "keywords": [
        "javascript",
        "node.js",
        "OSM",
        "OpenStreetMap",
        "protobuf",
        "pbf",
        "streaming",
        "parser"
    ],
    "author": "Boris Gontar",
    "license": "MIT",
    "dependencies": {
        "pbf": "github:junghans-schneider/pbf#master"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/borisgontar/osm-pbf-parser-node.git"
    },
    "homepage": "https://github.com/borisgontar/osm-pbf-parser-node",
    "devDependencies": {
        "@types/node": "^20.2.5",
        "@types/pbf": "^3.0.2",
        "rollup": "^3.23.0",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "typescript": "^5.0.4"
    }
}
